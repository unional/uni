{
  "preset": "ts-jest",
  "roots": [
    "<rootDir>/src"
  ],
  "collectCoverageFrom": [
    "<rootDir>/src/**/*.ts",
    "!<rootDir>/src/bin.ts",
    "!<rootDir>/src/commands/**/*"
  ],
  "testEnvironment": "node",
  "reporters": [
    "default",
    [
      "jest-junit",
      {
        "output": ".reports/junit/js-test-results.xml"
      }
    ]
  ]
}
