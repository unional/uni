{
  "name": "@unional/uni-cli",
  "version": "0.0.0-development",
  "description": "Development tool",
  "homepage": "https://github.com/unional/uni-cli",
  "bugs": {
    "url": "https://github.com/unional/uni-cli/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unional/uni-cli.git"
  },
  "license": "MIT",
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "rimraf lib",
    "dc": "npm run dependency-check",
    "dependency-check": "dependency-check . --unused --no-dev && dependency-check . --missing --no-dev",
    "lint": "tslint -p tsconfig.json",
    "test": "tsc && jest",
    "verify": "npm run lint && npm run build && jest --coverage && npm run test:browser && npm run dependency-check",
    "verify:ci": "npm run lint && npm run build && jest --coverage --config jest.config.ci.json",
    "watch": "jest --watch"
  },
  "dependencies": {
    "clibuilder": "^2.4.2",
    "make-error": "^1.3.5",
    "ncp": "^2.0.0",
    "ramda": "^0.26.1",
    "update-notifier": "^2.5.0"
  },
  "devDependencies": {
    "@types/ncp": "^2.0.1",
    "@types/ramda": "^0.25.44",
    "@types/tmp": "^0.0.33",
    "@unional/devpkg-node": "^1.0.1",
    "tmp": "^0.0.33"
  }
}
